{
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@grpc/reflection": "^1.0.4",
    "@payap/wallets": "*",
    "awilix": "^12.0.5"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "protoc": "^32.1.0",
    "ts-proto": "^2.7.7",
    "tsup": "^8.5.0"
  },
  "license": "ISC",
  "main": "build/main.js",
  "name": "@payap/transactions",
  "scripts": {
    "build": "tsup",
    "protoc": "protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --proto_path proto/ --ts_proto_opt=env=node,esModuleInterop=true,importSuffix=.ts,outputClientImpl=grpc-web,outputServices=grpc-js --ts_proto_out=source/generated/ $(find proto/ -type f -name '*.proto')",
    "start": "node build/main.js"
  },
  "type": "module"
}
